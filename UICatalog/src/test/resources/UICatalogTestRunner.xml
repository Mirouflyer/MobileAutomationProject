<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">

<suite name="iOS test suite on iPhone Simulator">

    <parameter name="OS" value="ios"/>
    <parameter name="appType" value="iPhone"/>
    <parameter name="deviceType" value="Simulator"/>
    <parameter name="deviceName" value="iPhone 7 Plus"/>
    <parameter name="version" value="10.2"/>
    <parameter name="moduleName" value="UiCatalog"/>
    <parameter name="appName" value="UICatalog6.1.app.zip"/>

    <test name="GoToHomePage" preserve-order="true">
        <classes>
            <class name="testHomePage.TestUIButtonsPage"></class>
            <class name="testHomePage.TestUIControlPage"></class>
            <class name="testHomePage.TestAlertsPage"></class>
            <class name="testHomePage.TestImagesPage"></class>
            <class name="testHomePage.TestPickersPage"></class>
            <class name="testHomePage.TestSearchBarPage"></class>
            <class name="testHomePage.TestSegmentsPage"></class>
            <class name="testHomePage.TestTextFieldsPage"></class>
            <class name="testHomePage.TestTextViewPage"></class>
            <class name="testHomePage.TestToolbarPage"></class>
            <class name="testHomePage.TestTransitionsPage"></class>
            <class name="testHomePage.TestWebPage"></class>
        </classes>
    </test>

</suite>